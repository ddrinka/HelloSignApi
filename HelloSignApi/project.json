{
  "language": "en",
  "version": "1.0.0-beta08",
  "title": "HelloSignApi",
  "description": "Unofficial task-based HelloSign api lib for dotnet with intellisense doc.",
  "authors": [ "Yin-Chun Wang" ],
  "copyright": "Copyright (C) Yin-Chun Wang 2016",
  "packOptions": {
    "licenseUrl": "https://raw.githubusercontent.com/soukoku/HelloSignApi/master/LICENSE",
    "projectUrl": "https://github.com/soukoku/HelloSignApi",
    "requireLicenseAcceptance": false,
    "tags": [ "eSignature" ]
  },
  "buildOptions": {
    "xmlDoc": true,
    "keyFile": "Sign.snk",
    "warningsAsErrors": true
  },
  "frameworks": {
    "net40-client": {
      "buildOptions": {
        "define": [ "FULLFX" ]
      },
      "dependencies": {
        "Newtonsoft.Json": "8.0.3",
        "Microsoft.Net.Http": "2.2.29"
      },
      "frameworkAssemblies": {
        "PresentationCore": "4.0.0.0",
        "System.Drawing": "4.0.0.0"
      }
    },
    "net45": {
      "buildOptions": {
        "define": [ "FULLFX" ]
      },
      "dependencies": {
        "Newtonsoft.Json": "8.0.3"
      },
      "frameworkAssemblies": {
        "PresentationCore": "4.0.0.0",
        "System.Drawing": "4.0.0.0",
        "System.Net.Http": "4.0.0.0"
      }
    },
    "netstandard1.3": {
      //"imports": "netcore",
      "dependencies": {
        "Newtonsoft.Json": "9.0.1-*",
        "System.Collections.Concurrent": "4.0.12-*",
        "System.IO": "4.1.0-*",
        "System.IO.FileSystem": "4.0.1-*",
        "System.IO.FileSystem.Primitives": "4.0.1-*",
        "System.Linq": "4.1.0-*",
        "System.Net.Http": "4.0.1-*",
        "System.Resources.ResourceManager": "4.0.1-*",
        "System.Runtime": "4.1.0-*",
        "System.Runtime.Extensions": "4.1.0-*",
        "System.Runtime.InteropServices": "4.1.0-*",
        "System.Security.Cryptography.Algorithms": "4.1.0-*",
        "System.Text.RegularExpressions": "4.0.12-*"
      }
    },
    ".NETPortable,Version=v4.0,Profile=Profile328": {
      "buildOptions": {
        "define": [ "PORTABLE" ]
      },
      "dependencies": {
        "Newtonsoft.Json": "8.0.3",
        "Microsoft.Net.Http": "2.2.29"
      }
    }
