{
  "language": "en",
  "version": "1.0.0-beta02",
  "title": "HelloSignApi",
  "description": "Simple task-based HelloSign api lib for dotnet with intellisense doc.",
  "authors": [ "Yin-Chun Wang" ],
  "copyright": "Copyright (C) Yin-Chun Wang 2016",
  "packOptions": {
    "licenseUrl": "https://raw.githubusercontent.com/soukoku/HelloSignApi/master/LICENSE",
    "projectUrl": "https://github.com/soukoku/HelloSignApi",
    "requireLicenseAcceptance": false,
    "tags": [ "signature" ]
  },
  "frameworks": {
    "net40-client": {
      "dependencies": {
        "Microsoft.Net.Http": "2.2.29"
      }
    },
    "net45": {
      "frameworkAssemblies": {
        "System.Net.Http": "4.0.0.0"
      }
    },
    ".NETPortable,Version=v4.0,Profile=Profile328": {
      "buildOptions": {
        "define": [ "PORTABLE" ]
      },
      "dependencies": {
        "Microsoft.Net.Http": "2.2.29"
      }
    },
    "netstandard1.3": {
      "imports": "dnxcore",
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-24027",
        "System.Security.Cryptography.Algorithms": "4.1.0-rc2-24027"
      }
    }
  },
  "buildOptions": {
    "xmlDoc": true,
    "keyFile": "Sign.snk"
  },
  "dependencies": {
    "Newtonsoft.Json": "8.0.3"
  }
}
